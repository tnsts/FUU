<div id="login" v-cloak>
  <div class="container">
    <form action="/executor/update" method="POST">
      <div class="form-group">
        <label>Email</label>
        <input type="email" class="form-control" name="mail" value="<%= executor.mail %>">
      </div>
      <div class="form-group">
        <label>Имя:</label>
        <input type="text" class="form-control" name="executorName" value="<%= executor.executorName %>">
        <br>
      </div>
      <div class="form-group">
        <label>Телефон:</label>
        <input type="phone" class="form-control" name="phone" value="<%= executor.phone %>">
        <br>
      </div>
      <input type="hidden" name="id" value="<%= executor.id %>">
      <button type="submit" class="btn btn-success">Save</button>
      <a href="/executor" class="btn btn-dark float-right">Get Back</a>
    </form>
  </div>
</div>
